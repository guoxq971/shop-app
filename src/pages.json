{
  "easycom": {
    "custom": {
      "^u--(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "^up-(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "^u-([^-].*)": "uview-plus/components/u-$1/u-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/share/index",
      "style": {
        "navigationBarTitleText": "share"
      }
    },
    {
      "path": "pages/shop/index",
      "style": {
        "navigationBarTitleText": "shop"
      }
    },
    {
      "path": "pages/new/index",
      "style": {
        "navigationBarTitleText": "new"
      }
    },
    {
      "path": "pages/cart/index",
      "style": {
        "navigationStyle": "custom" ,
        "navigationBarTitleText": "cart"
      }
    },
    {
      "path": "pages/me/index",
      "style": {
        "navigationBarTitleText": "heool"
      }
    }
  ],
  "subPackages": [
    {
      "root": "subPackages/share",
      "pages": [
        {
          "path": "test",
          "style": {
            "navigationBarTitleText": "test"
          }
        }
      ]
    }
  ],
  "globalStyle": {
    "navigationStyle": "custom",
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8",
    "app-plus": {
      "animationType": "fade-in",
      "animationDuration": "300"
    }
  },
  "tabBar": {
    "color": "#999999",
    "borderStyle": "black",
    "selectedColor": "#169bd5",
    "backgroundColor": "#ffffff",
    "list": [
      {
        "pagePath": "pages/share/index",
        "text": "Share",
        "iconPath": "static/tab_icons/home.png",
        "selectedIconPath": "static/tab_icons/home-active.png"
      },
      {
        "pagePath": "pages/shop/index",
        "text": "Shop",
        "iconPath": "static/tab_icons/cate.png",
        "selectedIconPath": "static/tab_icons/cate-active.png"
      },
      {
        "pagePath": "pages/new/index",
        "text": "New",
        "iconPath": "static/tab_icons/my.png",
        "selectedIconPath": "static/tab_icons/my-active.png"
      },
      {
        "pagePath": "pages/cart/index",
        "text": "Cart",
        "iconPath": "static/tab_icons/cart.png",
        "selectedIconPath": "static/tab_icons/cart-active.png"
      },
      {
        "pagePath": "pages/me/index",
        "text": "Me",
        "iconPath": "static/tab_icons/my.png",
        "selectedIconPath": "static/tab_icons/my-active.png"
      }
    ]
  }
}
